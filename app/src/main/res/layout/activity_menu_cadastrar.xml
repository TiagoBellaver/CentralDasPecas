<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".cadastrar.MenuCadastrarActivity">

    <ImageView
        android:id="@+id/voltarMenuPrincipal"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@image"
        android:onClick="voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.029999971"
        app:srcCompat="@mipmap/voltar" />

    <ImageView
        android:id="@+id/cadastroCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:contentDescription="@image"
        android:onClick="cadastrarcliente"
        app:layout_constraintEnd_toEndOf="@+id/cadastroFornecedor"
        app:layout_constraintStart_toStartOf="@+id/cadastroFornecedor"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/cliente" />

    <ImageView
        android:id="@+id/cadastroFornecedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:contentDescription="@image"
        app:layout_constraintEnd_toEndOf="@+id/cadastroProduto"
        app:layout_constraintStart_toStartOf="@+id/cadastroProduto"
        app:layout_constraintTop_toBottomOf="@+id/cadastroCliente"
        app:srcCompat="@mipmap/fornecedor" />

    <ImageView
        android:id="@+id/cadastroProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:contentDescription="@image"
        app:layout_constraintEnd_toEndOf="@+id/cadastroPedido"
        app:layout_constraintStart_toStartOf="@+id/cadastroPedido"
        app:layout_constraintTop_toBottomOf="@+id/cadastroFornecedor"
        app:srcCompat="@mipmap/produtos" />

    <ImageView
        android:id="@+id/cadastroPedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:contentDescription="@image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cadastroProduto"
        app:srcCompat="@mipmap/pedido" />
</android.support.constraint.ConstraintLayout>